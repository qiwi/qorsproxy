{
  "name": "qorsproxy",
  "description": "Cors proxy for Qoder/Pusher",
  "keywords": [
    "cors",
    "corsproxy",
    "qors",
    "qorsproxy"
  ],
  "version": "1.5.0",
  "repository": {
    "type": "git",
    "url": "git@github.com:qiwi/qorsproxy.git"
  },
  "author": "Anton Golub <a.golub@qiwi.com>",
  "engines": {
    "node": ">=9.4.0",
    "npm": ">=5.6"
  },
  "dependencies": {
    "@qiwi/health-indicator": "^1.2.2",
    "@qiwi/primitive-storage": "^1.2.1",
    "async-middleware": "^1.2.1",
    "basic-auth": "^2.0.1",
    "esm": "3.2.25",
    "express": "^4.17.1",
    "ip": "^1.1.5",
    "jsonschema": "^1.2.4",
    "lodash-es": "^4.17.11",
    "request": "^2.88.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.9.0",
    "yargs": "^13.2.4"
  },
  "esm": {
    "mode": "all",
    "cjs": true
  },
  "main": "src/app",
  "scripts": {
    "start_pm2": "pm2 start npm --name qorsproxy -- start",
    "start": "node -r esm --use_strict src/app",
    "test": "npm run mocha",
    "mocha": "nyc mocha --opts .mocha.opts && nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "test_with_report": "npm test && npm run coveralls_push",
    "coveralls_push": "cat ./coverage/lcov.info | coveralls"
  },
  "license": "MIT",
  "files": [
    "README.md",
    "CHANGELOG.md",
    "src/"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.15",
    "@semantic-release/github": "^5.4.0",
    "@semantic-release/npm": "^5.1.13",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.0.4",
    "glob": "^7.1.4",
    "mocha": "^6.1.4",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-sinon": "^2.1.0",
    "nyc": "^13.3.0",
    "proxyquire": "^2.1.0",
    "reqresnext": "^1.5.1",
    "semantic-release": "^15.13.18",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  }
}
