{
  "name": "qorsproxy",
  "version": "2.0.1",
  "description": "Cors proxy for dev purposes",
  "main": "src/main/js/index.js",
  "bin": {
    "qorsproxy": "src/main/js/index.js"
  },
  "scripts": {
    "start:pm2": "pm2 start npm --name qorsproxy -- start",
    "start": "node --use_strict src/main/js/index.js",
    "test": "npm run lint && npm run mocha",
    "lint": "standard src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "mocha": "nyc mocha --opts .mocha.opts && nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "test:report": "npm test && npm run coveralls:push",
    "coveralls:push": "cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "@qiwi/health-indicator": "^1.2.3",
    "@qiwi/primitive-storage": "^1.3.0",
    "async-middleware": "^1.2.1",
    "basic-auth": "^2.0.1",
    "esm": "3.2.25",
    "express": "^4.17.1",
    "http-status-codes": "^1.3.2",
    "ip": "^1.1.5",
    "jsonschema": "^1.2.4",
    "lodash-es": "^4.17.15",
    "meow": "^5.0.0",
    "request": "^2.88.0",
    "winston": "^3.2.1",
    "winston-daily-rotate-file": "^3.10.0"
  },
  "esm": {
    "mode": "all",
    "cjs": true
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src/main",
    "example"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^3.0.4",
    "@semantic-release/git": "^7.0.16",
    "@semantic-release/github": "^5.4.3",
    "@semantic-release/npm": "^5.1.15",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.0.6",
    "dirty-chai": "^2.0.1",
    "glob": "^7.1.4",
    "mocha": "^6.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-sinon": "^2.1.0",
    "nyc": "^14.1.1",
    "proxyquire": "^2.1.3",
    "reqresnext": "^1.5.4",
    "semantic-release": "^15.13.24",
    "sinon": "^7.4.1",
    "sinon-chai": "^3.3.0",
    "standard": "^14.1.0"
  },
  "keywords": [
    "cors",
    "corsproxy",
    "qors",
    "qorsproxy"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:qiwi/qorsproxy.git"
  },
  "author": "Anton Golub <a.golub@qiwi.com>",
  "engines": {
    "node": ">=9.4.0",
    "npm": ">=5.6"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "standard": {
    "ignore": [
      "src/main/js/index.js"
    ],
    "globals": [
      "it",
      "expect",
      "describe",
      "before",
      "after",
      "afterEach",
      "beforeEach",
      "sinon"
    ]
  }
}
