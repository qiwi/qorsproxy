{
  "name": "qorsproxy",
  "version": "2.4.1",
  "description": "Cors proxy for dev purposes",
  "main": "src/main/js/index.js",
  "bin": {
    "qorsproxy": "src/main/js/index.js"
  },
  "scripts": {
    "start:pm2": "pm2 start npm --name qorsproxy -- start",
    "start": "node --use_strict src/main/js/index.js",
    "test": "npm run lint && npm run mocha",
    "lint": "standard src/**/*.js",
    "lint:fix": "npm run lint -- --fix",
    "mocha": "nyc mocha ./src/test/js/**/*.js && nyc report --reporter=text-lcov > ./coverage/lcov.info",
    "test:report": "npm test && npm run coveralls:push",
    "coveralls:push": "cat ./coverage/lcov.info | coveralls",
    "postupdate": "npm i && npm audit fix && npm test",
    "release": "semantic-release"
  },
  "dependencies": {
    "@qiwi/health-indicator": "^1.3.3",
    "@qiwi/primitive-storage": "^1.4.4",
    "async-middleware": "^1.2.1",
    "basic-auth": "^2.0.1",
    "esm": "3.2.25",
    "express": "^4.17.1",
    "http-status-codes": "^2.1.4",
    "ip": "^1.1.5",
    "jsonschema": "^1.2.7",
    "lodash-es": "^4.17.15",
    "meow": "^8.0.0",
    "pkg-dir": "^4.2.0",
    "request": "^2.88.2",
    "winston": "^3.3.3",
    "winston-daily-rotate-file": "^4.5.0"
  },
  "esm": {
    "cjs": true
  },
  "license": "MIT",
  "files": [
    "LICENSE",
    "README.md",
    "CHANGELOG.md",
    "src/main",
    "example",
    "ssl"
  ],
  "devDependencies": {
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/github": "^7.1.1",
    "@semantic-release/npm": "^7.0.6",
    "chai": "^4.2.0",
    "chai-subset": "^1.6.0",
    "coveralls": "^3.1.0",
    "dirty-chai": "^2.0.1",
    "glob": "^7.1.6",
    "mocha": "^8.1.3",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-sinon": "^2.1.2",
    "nyc": "^15.1.0",
    "proxyquire": "^2.1.3",
    "reqresnext": "^1.6.3",
    "semantic-release": "^17.1.2",
    "sinon": "^9.1.0",
    "sinon-chai": "^3.5.0",
    "standard": "^14.3.4"
  },
  "keywords": [
    "cors",
    "corsproxy",
    "qors",
    "qorsproxy"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:qiwi/qorsproxy.git"
  },
  "author": "Anton Golub <a.golub@qiwi.com>",
  "engines": {
    "node": ">=9.4.0",
    "npm": ">=5.6"
  },
  "release": {
    "branch": "master",
    "verifyConditions": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "prepare": [
      "@semantic-release/changelog",
      "@semantic-release/npm",
      "@semantic-release/git"
    ],
    "publish": [
      "@semantic-release/npm",
      "@semantic-release/github"
    ]
  },
  "standard": {
    "ignore": [
      "src/main/js/index.js"
    ],
    "globals": [
      "it",
      "expect",
      "describe",
      "before",
      "after",
      "afterEach",
      "beforeEach",
      "sinon"
    ]
  }
}
