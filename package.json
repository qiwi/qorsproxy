{
  "name": "@qiwi/qorsproxy",
  "alias": "qorsproxy",
  "version": "4.0.1",
  "description": "HTTP(S) proxy for dev purposes",
  "type": "module",
  "types": "./src/main/js/index.d.ts",
  "exports": {
    ".": {
      "types": "./src/main/js/index.d.ts",
      "import": "./src/main/js/index.js"
    }
  },
  "bin": {
    "qorsproxy": "./src/main/js/cli.js"
  },
  "scripts": {
    "start:pm2": "pm2 start npm --name qorsproxy -- start",
    "start": "node --use_strict src/main/js/cli.js",
    "start:bun": "bun src/main/js/cli.js",
    "build": "exit 0",
    "build:docs": "mkdir -p docs && cp -r README.md docs/README.md",
    "test": "concurrently -m 2 --kill-others-on-fail 'npm:test:*'",
    "test:unit": "mkdir -p .nyc_output && c8 mocha './src/test/js/**/*.test.js' && c8 report -r lcov",
    "test:lint": "eslint -c .eslintrc.json src/**/*.js",
    "test:depcheck": "npm_config_yes=true npx depcheck --ignores mocha-*",
    "test:depaudit": "yarn audit --scope=dependencies --level=moderate",
    "lint:fix": "yarn lint -- --fix",
    "publish:beta": "npm publish --no-git-tag-version --tag beta",
    "publish:rc": "npm publish --no-git-tag-version --tag rc"
  },
  "dependencies": {
    "@qiwi/health-indicator": "^2.0.3",
    "@qiwi/primitive-storage": "^2.0.1",
    "basic-auth": "^2.0.1",
    "express": "^4.18.2",
    "http-status-codes": "^2.2.0",
    "ip": "^2.0.0",
    "js-yaml": "^4.1.0",
    "jsonschema": "^1.4.1",
    "lodash-es": "^4.17.21",
    "meow": "^12.0.1",
    "undici": "^5.22.1",
    "winston": "^3.10.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "license": "MIT",
  "files": [
    "src/main",
    "example",
    "ssl"
  ],
  "devDependencies": {
    "c8": "^8.0.0",
    "chai": "^4.3.7",
    "chai-subset": "^1.6.0",
    "concurrently": "^8.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^8.44.0",
    "eslint-config-qiwi": "^2.1.3",
    "mocha": "^10.2.0",
    "mocha-lcov-reporter": "^1.3.0",
    "mocha-sinon": "^2.1.2",
    "reqresnext": "^1.7.0",
    "sinon": "^16.0.0",
    "sinon-chai": "^3.7.0",
    "tempy": "^3.1.0"
  },
  "keywords": [
    "cors",
    "corsproxy",
    "qors",
    "qorsproxy"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:qiwi/qorsproxy.git"
  },
  "author": "Anton Golub <a.golub@qiwi.com>",
  "engines": {
    "node": ">=16",
    "npm": ">=5.6"
  }
}
